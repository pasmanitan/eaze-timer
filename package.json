{
  "name": "eaze-timer",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "clean": "rm -rf dist && echo 'cleaned!'",
    "dist": "bun clean && bun build ./src/index.html --minify --outdir ./dist --splitting --target browser && echo 'built prod files!'",
    "prod": "bun dist && bun run ./dist/index.html",
    "dev": "bun run ./src/index.html"
  }
}
